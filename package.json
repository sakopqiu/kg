  "scripts": {
    "start": "webpack-dev-server --open --progress --colors --config webpack.dev.js",
    "start2": "node --max_old_space_size=4096 ./node_modules/.bin/webpack-dev-server --open --progress --colors --config webpack.dev.js",
    "sreact": "cd src/simple-react && webpack-dev-server --open --progress --colors",
    "dev-fast": "SOURCE_MAP=false webpack-dev-server --open --progress --colors --config webpack.dev.js",
    "build-module": "rimraf -rf lib dist && npm run lint && npm run gend && npm run copy-resource-files",
    "compile-all-files": "npx tsc > /dev/null 2>&1",
    "compile-all-files-for-release": "npx tsc --sourceMap false --inlineSources false --inlineSourceMap false > /dev/null 2>&1",
    "copy-resource-files": "node ./scripts/copy-resource-files.js",
    "gend": "npx tsc --skipLibCheck --allowJs false -d --declarationDir lib",
    "test": "jest --config .jest.js",
    "lint": "npx tslint -c ./webpack/tslint.json ./{src,test_cases}/**/*{.ts,.tsx} && npx stylelint --config ./webpack/.stylelintrc -s scss ./{src,test_cases}/**/*.scss",
    "autofix": "npx tslint --fix -c ./webpack/tslint.json ./{src,test_cases}/**/*{.ts,.tsx} && npx stylelint --config ./webpack/.stylelintrc -s scss ./{src,test_cases}/**/*.scss --fix"
  },
